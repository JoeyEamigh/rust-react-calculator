{
  "singleQuote": true,
  "semi": true,
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.js",
        "*.tsx",
        "*.jsx"
      ],
      "options": {
        "printWidth": 120
      }
    },
    {
      "files": [
        "*.html"
      ],
      "options": {
        "printWidth": 100
      }
    }
  ]
}